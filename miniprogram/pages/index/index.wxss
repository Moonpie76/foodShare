@font-face {
    font-family: 'iconfont';
    /* Project id 2525554 */
    src: url('//at.alicdn.com/t/font_2525554_c9e4z0ixhm4.woff2?t=1620047771974') format('woff2'),
        url('//at.alicdn.com/t/font_2525554_c9e4z0ixhm4.woff?t=1620047771974') format('woff'),
        url('//at.alicdn.com/t/font_2525554_c9e4z0ixhm4.ttf?t=1620047771974') format('truetype');
}

.top {
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    width: 100%;
    height: 100%;
}

.top_lead {
    display: flex;
    justify-content: space-around;
    flex-direction: row;
    align-items: center;
    width: 100%;
    height: 70rpx;
}

.note {
    display: flex;
    justify-content: space-around;
    align-items: center;
    margin-top: 1%;
    width: 98%;
    height: 100%;
}

.lead {
    display: flex;
    width: 15%;
    height: 100%;
    margin-right: 10rpx;
    align-items: center;
    justify-content: space-around;
}

.locate {
    display: flex;
    width: 100%;
    height: 100%;
    font-family: "iconfont";
    font-size: 70rpx;
    font-style: normal;
    justify-content: center;
    color: rgb(255, 60, 0);
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-locate:before {
    content: '\e60d';
}

.search {
    display: flex;
    width: 100%;
    height: 100%;
    justify-content: space-around;
    align-items: center;
}