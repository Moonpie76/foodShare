var checkGood = function (list, length, id) {
    for (var i = 0; i <= length; i++) {
        if (list[i] == id) {
            return '1';
        } 
    }
    return '0';
}
var checkCollection = function (list, length, id) {
    for (var i = 0; i <= length; i++) {
        if (list[i] == id) {
            return '1';
        } 
    }
    return '0';
}
module.exports.checkGood = checkGood;
module.exports.checkCollection = checkCollection;