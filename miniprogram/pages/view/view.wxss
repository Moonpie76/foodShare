.host{
  width:100%;
  margin:2px;
  line-height: 40px;
  display: flex;
}
.profile{
  width:40px;
  height:40px;
  margin-left: 5px;
  align-items: center;
  display: flex;
  justify-content: center;
}
.avatar{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.name{
  margin-left: 20px;
}
.photo{
  width: 100%;
}
#photo{
  align-items: center;
  display: flex;
  justify-content: center;
}
.title{
  margin: 10px;
  margin-top:20px;
  font-weight: 550;
  font-family: sans-serif;
  
}
.detail{
  margin: 10px;
  font-family: fantasy;
  font-size:35rpx;
}
.timelocation{
  display: flex;
  font-size: 30rpx;
  margin:10px;
  color: darkgray;
  margin-right: 20px;
  justify-content: space-between;
}
.line{
  margin: 10px;
  border-top:1px #eeeaea solid;
}
.grade{
  margin:10px;
  font-style: inherit;
  display: flex;
}
.handel{
  margin:5px;
  width: 100%;
  display: flex;
  line-height: 40px;
}
#comment{
  width: 100px;
  background-color: lightsalmon;
}
#like{
  width: 40px;
  background-color: lightseagreen;
  margin-left: 50px;
}
#review{
  width: 40px;
  margin-left: 20px;
  background-color: lightseagreen;
}
#collect{
  width: 40px;
  margin-left: 20px;
  background-color: lightseagreen;
}
.comment{
  margin:5px;
}
@font-face {
  font-family: 'iconfont';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'),
      url('data:font/woff;charset=utf-8;base64,') format('woff'),
      url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fanhui1:before {
  content: "\e601";
}

.icon-fanhui:before {
  content: "\e604";
}

.star{
  height: 60rpx;
  width: 60rpx;
  display: inline-block;
}